  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true npm run test:engine && npm run lint",
    "test:engine": "check-engine",
    "test:sec": "npm audit || true",
    "test:unit": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write '**/*.{js,jsx,json,md}'",
    "format:changed": "pretty-quick",
    "format:staged": "pretty-quick --staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "npm run lint:js && npm run lint:dup && npm run lint:css",
    "lint-fix": "eslint --fix",
    "clean": "rm -rf dist",
    "lint-fix:js": "eslint src/**/*.js -c .eslintrc.json --fix",
    "lint:js": "eslint src/**/*.js -c .eslintrc.json",
    "lint:css": "stylelint src/**/*.{scss,css} && stylelint --rd src/**/*.{scss,css}",
    "lint:dup": "jscpd -g --threshold=1 client tests docker",
    "ci": "npm run test && npm run build && npm run test:sec"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "npm": ">=6",
    "node": ">=8"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not ie < 11",
    "not ie_mob < 11",
    "not dead"
